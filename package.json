{
  "name": "hodlwatch",
  "homepage": "https://hodl.watch",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/belaczek/hodlwatch.git"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start:production": "serve -s build",
    "build": "npm-run-all build-css build-js",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "npm test -- --coverage",
    "analyze": "source-map-explorer build/static/js/main.*",
    "format": "prettier-eslint --write 'src/**/*.js'",
    "format:single": "prettier-eslint --write",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "predeploy": "npm run build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "prebuild": "cp webpack.config.prod.js node_modules/react-scripts/config"
  },
  "lint-staged": {
    "*.js": [
      "format:single",
      "eslint --max-warnings=0",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "big.js": "^5.0.3",
    "bloomer": "^0.6.3",
    "bulma": "^0.6.2",
    "bulma-switch": "^1.0.1",
    "ccxt": "^1.12.117",
    "classnames": "^2.2.5",
    "color-hash": "^1.0.3",
    "crypto-js": "^3.1.9-1",
    "date-fns": "^1.29.0",
    "lodash": "^4.17.5",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-toastify": "^3.4.3",
    "recharts": "^1.0.0-beta.10",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "jest-cli": "^22.4.3",
    "lint-staged": "^6.1.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-scripts": "1.1.1",
    "serve": "^6.5.4",
    "source-map-explorer": "^1.5.0",
    "uglifyjs-webpack-plugin": "^1.2.4"
  }
}
