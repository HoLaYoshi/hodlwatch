{
  "name": "hodlwatch",
  "homepage": "https://hodl.watch",
  "version": "0.2.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/belaczek/hodlwatch.git"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "serve dist",
    "dev": "next src",
    "build": "next build src && npm run export",
    "export": "next export src -o './dist'",
    "deploy": "gh-pages -d dist",
    "test": "cross-env test=1 jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "format": "prettier-eslint --write 'src/**/*.js'",
    "format:single": "prettier-eslint --write",
    "predeploy": "npm run export",
    "precommit": "lint-staged",
    "analyze": "cross-env BUNDLE_ANALYZE=browser next build src"
  },
  "lint-staged": {
    "*.js": [
      "npm run format:single --",
      "eslint --max-warnings=0",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "big.js": "^5.0.3",
    "bloomer": "^0.6.3",
    "bulma": "^0.7.1",
    "bulma-switch": "^1.0.2",
    "ccxt": "^1.17.321",
    "classnames": "^2.2.6",
    "color-hash": "^1.0.3",
    "crypto-js": "^3.1.9-1",
    "date-fns": "^1.29.0",
    "lodash": "^4.17.11",
    "next": "^7.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-toastify": "^3.4.3",
    "recharts": "^1.2.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "serve": "^10"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@types/jest": "^23.3.2",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-sass": "^1.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.1.0",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  }
}
